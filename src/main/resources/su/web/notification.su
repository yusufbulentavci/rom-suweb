--@DEPENDS:.base,.prosedur



--@TABLE uzanti=not
create table web.notification(
	id bigserial primary key,
	role text not null,
	prosedur_id bigint references web.prosedur(id) not null,
	creation_time timestamp,
	expire_time timestamp,
	token text,
	params jsonb,
	declaration jsonb,
	report jsonb,
	accomplished timestamp,
	result boolean,
	prosedur_type text,
	func text,
	client_app text
  );
--@END

